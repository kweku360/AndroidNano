<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kfive.com.androidnano">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NanoHome" />
        <activity android:name=".SwipeCarder.SwipeCarderActivity" />
        <activity android:name=".AppLister.ApplistMain" />
        <activity android:name=".AppVault.AppVaultMain" />
        <activity android:name=".budplayer.BmPlayer" />
        <activity android:name=".RxAndroid.RxTest" />
        <activity android:name=".Animations.AnimDrawing" />
        <activity android:name=".Animations.Animations">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".firebase.SimpleSignin"></activity>
        <activity android:name=".firebase.AdminSection"></activity>
    </application>

</manifest>